[core]
    editor = vim
[user]
    name = Alessandro DiMarco
    email = 8059120+alesso-x@users.noreply.github.com
[alias]
    b = branch
    c = checkout
    diff-origin = diff master origin/master
    diff-staged = diff --cached
    log-graph = log --oneline --graph --decorate
    log-origin = log origin/master ^master
    ls-alias = "!git config -l | grep alias | cut -c 7-"
    publish = "!git push -u origin $(git branch-name)"
    s = switch
    undo = reset HEAD~
[diff "sopsdiffer"]
    textconv = sops -d
[pull]
    ff = only
[init]
    defaultBranch = main
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
