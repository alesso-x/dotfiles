{
  "docker-compose init": {
    "prefix": "docker-init",
    "body": [
      "version: '3.8'\n",
      "services:",
      "\t$0\n",
      "volumes:",
      "\tdb_data:",
    ],
    "description": "docker-compose init"
  },
  "docker-compose backend": {
    "prefix": "docker-backend",
    "body": [
      "backend:",
      "\tbuild:",
      "\t\tcontext: .",
      "\t\tdockerfile: ./config/backend.Dockerfile",
      "\tcommand: $1",
      "\tenv_file:",
      "\t\t- ./.env.local",
      "\tvolumes:",
      "\t\t- .:/usr/src/app",
      "\t\t- /usr/src/app/node_modules/",
      "\tports: [\"4000:4000\"]",
      "\tdepends_on:",
      "\t\t- db",
      "$0",
    ],
    "description": "docker-compose backend"
  },
  "docker-compose frontend": {
    "prefix": "docker-frontend",
    "body": [
      "frontend:",
      "\tbuild:",
      "\t\tcontext: .",
      "\t\tdockerfile: ./config/frontend.Dockerfile",
      "\tcommand: $1",
      "\tenv_file:",
      "\t\t- ./.env.local",
      "\tvolumes:",
      "\t\t- .:/usr/src/app",
      "\t\t- /usr/src/app/node_modules/",
      "\tports: [\"3000:3000\"]",
      "$0",
    ],
    "description": "docker-compose frontend"
  },
  "docker-compose db": {
    "prefix": "docker-db",
    "body": [
      "db:",
      "\timage: postgres",
      "\tports: [\"5432:5432\"]",
      "\tvolumes:",
      "\t\t- db_data:/var/lib/postgresql/data",
      "\tenvironment:",
      "\t\tPOSTGRES_USER: \"pguser\"",
      "\t\tPOSTGRES_PASSWORD: \"pgpass\"",
      "\t\tPOSTGRES_DB: \"$1\"",
      "$0",
    ],
    "description": "docker-compose db"
  },
}
